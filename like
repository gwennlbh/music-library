#!/usr/bin/env bash
DELIMITER=:
artist=$(echo $@ | awk -F"$DELIMITER" '{print $1}' | sed 's/^\s*\|\s*$//g')
title=$(echo $@ | awk -F"$DELIMITER" '{print $2}' | sed 's/^\s*\|\s*$//g')
echo Saving $artist â€” $title to library...
echo -e "$artist\t$title" >> $HOME/music/library.tsv
$HOME/music/backup
